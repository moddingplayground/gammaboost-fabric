{
  "schemaVersion": 1,

  "id": "gammaboost",
  "version": "${version}",
  "license": [
    "MIT (Code)",
    "All Rights Reserved (Assets)"
  ],

  "name": "Gamma Boost",
  "icon": "assets/gammaboost/icon.png",
  "description": "A Minecraft mod for boosting gamma in certain contexts",

  "authors": [
    "Gaetano Russo (Ninni)",
    "Daniel Orr (Andante)"
  ],
  "contact": {
    "homepage": "https://moddingplayground.net/gammaboost",
    "sources": "https://github.com/moddingplayground/gammaboost-fabric",
    "issues": "https://github.com/moddingplayground/gammaboost-fabric/issues"
  },
  "custom": {
    "modmenu": { "links": { "mod.gammaboost.contact.discord": "https://discord.moddingplayground.net" } }
  },

  "environment": "client",
