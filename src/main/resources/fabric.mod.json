    "client": [ "net.moddingplayground.gammaboost.impl.GammaBoostClientImpl" ]
  },
  "mixins": [ "gammaboost.mixins.json" ],

  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19",
    "fabricloader": ">=0.14",
    "fabric": ">=0.57",
    "frame": ">=0.20"
  }
